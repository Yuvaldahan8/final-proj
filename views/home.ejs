<div class="parallax-container">
    <% [
        {src: "https://images.unsplash.com/photo-1599334064100-4d3a0cc7332c?q=80&w=2535&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D", alt: "Image 1", title: "Welcome to Our Nursery", description: "Discover a world of beautiful plants"},
        {src: "https://images.unsplash.com/photo-1551558842-4a84f7e3b5ab?q=80&w=2670&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D", alt: "Image 2", title: "Our Collection", description: "Explore our wide variety of plants"},
        {src: "https://images.unsplash.com/photo-1578687595593-31fafb682273?q=80&w=2670&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D", alt: "Image 3", title: "Expert Care", description: "We provide the best care for all our plants"}
        
    ].forEach((image, index) => { %>
        <section class="parallax-section" id="section-<%= index %>">
            <div class="image-container">
                <img src="<%= image.src %>" alt="<%= image.alt %>" class="parallax-image">
            </div>
            <div class="content">
                <h2><%= image.title %></h2>
                <p><%= image.description %></p>
            </div>
        </section>
    <% }); %>
</div>

